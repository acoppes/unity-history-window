.favorite {
    font-size: 12px;
    -unity-text-align: middle-left;
    margin: 2px;
    padding: 3px;
    background-color: #282828;
    border-color: black;
    border-width: 1px;
    border-radius: 3px;
}

.favorite #DragArea{
    margin-right: 16px;
}

.favorite Label {
    margin-left: 17px;
}

.favorite #Icon {
    max-width: 16px;
    max-height: 16px;
    visibility: initial;
    position: absolute;
    align-self: flex-start;
}

.favorite #RemoveIcon {
    max-width: 16px;
    max-height: 16px;
    right: 4px;
    position: absolute;
    /* align-self: flex-end; */
}

.favorite #OpenPrefabIcon {
    max-width: 16px;
    max-height: 16px;
    right: 30px;
    position: absolute;
    /* align-self: flex-end; */
}

